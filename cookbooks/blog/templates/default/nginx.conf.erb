server {
        listen <%= @server_port %>;
        server_name <%= @server_name %> <%= @server_aliases.join(" ") %>;

        access_log   /var/log/nginx/<%= @server_name %>.access.log;
        error_log    /var/log/nginx/<%= @server_name %>.error.log;

        root <%= @docroot %>;
        index index.php;

        location / {
                try_files $uri $uri/ /index.php?$args;
        }

        include hhvm.conf;
}
